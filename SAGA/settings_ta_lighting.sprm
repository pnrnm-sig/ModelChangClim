  <OPTION type="double" id="ATMOSPHERE" name="Height of Atmosphere [m]">12000</OPTION>
  <OPTION type="double" id="PRESSURE" name="Barometric Pressure [mbar]">1013</OPTION>
  <OPTION type="double" id="WATER" name="Water Content [cm]">1.68</OPTION>
  <OPTION type="double" id="DUST" name="Dust [ppm]">100</OPTION>
  <OPTION type="double" id="LUMPED" name="Lumped Atmospheric Transmittance [Percent]">70</OPTION>
  <OPTION type="choice" id="UPDATE" name="Update" index="0">do not update</OPTION>
  <OPTION type="double" id="UPDATE_STRETCH" name="Constant Histogram Stretch">1</OPTION>
</parameters>
